<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name=viewport content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=0">
  <meta name="google-site-verification" content="mhqdpp_Xwfs-HeZvF6fQ1OR-pq3wNylaet4dVvUeLPk" />
  <link href="https://fonts.googleapis.com/css?family=Montserrat:400,400i,600,700,700i,900" rel="stylesheet">
  <link href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
  <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
  <link rel="manifest" href="/site.webmanifest">
  <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
  <meta name="msapplication-TileColor" content="#da532c">
  <meta name="theme-color" content="#ffffff">
  <title>UÂ°Community</title>

  <% if (typeof contentMetaTags === 'object') { %>
    <% if (typeof contentMetaTags.path === 'string' ) { %>
      <meta property="og:url" content="https://u.community<%- contentMetaTags.path %>" />
    <% } %>
    <% if (typeof contentMetaTags.type === 'string' ) { %>
      <meta property="og:type" content="<%- contentMetaTags.type %>" />
    <% } %>
    <% if (typeof contentMetaTags.title === 'string' ) { %>
      <meta property="og:title" content="<%- contentMetaTags.title %>" />
    <% } %>
    <% if (typeof contentMetaTags.description === 'string' ) { %>
      <meta property="og:description" content="<%- contentMetaTags.description %>" />
    <% } %>
    <% if (typeof contentMetaTags.image === 'string' ) { %>
      <meta property="og:image" content="<%- contentMetaTags.image %>" />
    <% } %>
  <% } %>

  <style>
    .no-js {
      display: none;
    }
  </style>
</head>
<body class="no-js">
  <div id="app"><%- content %></div>

  <!-- Google Tag Manager (noscript) -->
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-WGSSH3Q" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->

  <script>
    window.APP_STATE = <%- JSON.stringify(state) %>;
  </script>

  <script type="text/javascript" src="/app.js?<%- staticVersion %>"></script>
</body>
</html>
